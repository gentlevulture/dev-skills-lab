<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dev Skills Lab</title>
</head>
<body>
  <h1>Dev Skills To Develop</h1>
  <% if(error) { %>
    <h2><%= error %></h2>
  <% } %>
  <ul>
    <% todos.forEach(function(todo) { %>
      <li>
        <a href="/todos/<%= todo._id %>"><%= todo.text %></a>
          - 
        <%= todo.done ? 'developed' : 'not developed' %>
      </li>
    <% }) %>
  </ul>
</body>
</html>